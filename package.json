{
  "name": "@kninnug/containing-triangle",
  "version": "1.1.1",
  "description": "Find the triangle of a Delaunator triangulation that contains a given point",
  "main": "./lib/containing-triangle.cjs",
  "module": "./lib/containing-triangle.mjs",
  "exports": {
    "import": "./lib/containing-triangle.mjs",
    "require": "./lib/containing-triangle.cjs"
  },
  "types": "./containing-triangle.ts",
  "files": [
    "containing-triangle.ts",
    "lib/containing-triangle.mjs",
    "lib/containing-triangle.cjs",
    "lib/containing-triangle.js",
    "lib/containing-triangle.min.js"
  ],
  "scripts": {
    "test": "npm run build && node test/test.mjs",
    "build": "rollup -c",
    "prepare": "npm run build",
    "clean": "rm -r ./coverage ./lib/ ./test/ || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kninnug/containing-triangle-js.git"
  },
  "keywords": [
    "delaunay",
    "triangulation",
    "containing",
    "triangle",
    "Delaunator"
  ],
  "author": "Marco Gunnink",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kninnug/containing-triangle-js/issues"
  },
  "homepage": "https://github.com/kninnug/containing-triangle-js#readme",
  "directories": {
    "lib": "./lib/"
  },
  "dependencies": {
    "robust-predicates": "^3.0.1"
  },
  "devDependencies": {
    "@kninnug/constrainautor": "^3.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/delaunator": "^5.0.0",
    "@types/tape": "^4.13.2",
    "delaunator": "^5.0.0",
    "rollup": "^2.48.0",
    "rollup-plugin-terser": "^7.0.2",
    "tape": "^5.2.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
